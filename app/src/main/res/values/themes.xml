<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.DeliveryAggregator" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.DeliveryAggregator.Launcher" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@android:color/white</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/splash_logo</item>
        <item name="postSplashScreenTheme">@style/Theme.DeliveryAggregator</item>
    </style>
</resources>